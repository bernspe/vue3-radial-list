{
  "name": "vue3-radial-list",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": ["dist", "src/components/"],
  "keywords": [
    "list",
    "circular list",
    "radial list",
    "circular menu",
    "radial menu",
    "pie menu",
    "pie list",
    "vue3 component",
    "typescript",
    "vue3",
    "vue",
    "vuejs"
  ],
  "repository": {
  "type": "git",
  "url": "git+https://github.com/bernspe/vue3-radial-list.git"
},
  "description": "Vue3 + TypeScript Radial list component",
  "main": "dist/vue3-radial-list.umd.cjs",
  "module": "dist/vue3-radial-list.js",
  "exports": {
    ".": {
      "style": "./dist/vue3-radial-list.css",
      "types": "./dist/index.d.ts",
      "import": "./dist/vue3-radial-list.js",
      "require": "./dist/vue3-radial-list.umd.cjs"
    },
    "./dist/vue3-radial-list.css": {
      "import": "./dist/vue3-radial-list.css",
      "require": "./dist/vue3-radial-list.css"
    }
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
